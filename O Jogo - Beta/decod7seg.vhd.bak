library IEEE;
use IEEE.Std_Logic_1164.all;
entity decod7seg is
port (C: in std_logic_vector(3 downto 0);
		F: out std_logic_vector(6 downto 0)
);
end decod7seg;

architecture arqDecod of decod7seg is
begin
	F <= 	"0000" when C = "1000000" else
			"0001" when C = "1111001" else
			"0010" when C = "0100100" else
		   "0011" when C = "0110000" else
			"0100" when C = "0011001" else
			"0101" when C = "0010010" else
			"0110" when C = "0000010" else
			"0111" when C = "1111000" else
			"1000" when C = "0000000" else
			"1001" when C = "0010000" else
			"1010" when C = "1000111" else
			"1000000";
			
											
end decod;